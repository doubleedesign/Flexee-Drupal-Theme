{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
	'dark-section',
  ]
%}
{% if content %}
	<footer id="page-footer" role="contentinfo" {{ attributes.addClass(classes) }}>
	  <div class="row">
		  {{ content }}
	  </div>
	</footer>
	<div id="subfooter" class="dark-section">
		<div class="row justify-right">
			<div class="columns">
				<small>Website by <a href="https://www.doubleedesign.com.au">Double-E Design</a>.</small>
			</div>
		</div>
	</div>
{% endif %}
